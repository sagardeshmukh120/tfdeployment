pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                build = build.previousBuild
                sh '''#!/bin/bash
                echo $Action
                echo $No_of_appserver
                echo $No_of_webserver
                              
                echo $No_of_appserver
                echo $build
                '''
            }
        }
    }
}


